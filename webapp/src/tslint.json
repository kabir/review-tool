{
    "extends": "../tslint.json",
    "rules": {
      "directive-selector": [
        true,
        "attribute",
        "app",
        "camelCase"
      ],
      "component-selector": [
        true,
        "element",
        "app",
        "kebab-case"
      ],
      "no-string-literal": false,
      "variable-name": ["ban-keywords", "check-format", "allow-pascal-case", "allow-leading-underscore"]
    }
}
